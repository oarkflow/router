{
    "routes": [
        {
            "route_uri": "/test-route",
            "route_method": "POST",
            "request": {
                "type": "object",
                "description": "users",
                "required": [ "user_id" ],
                "properties": {
                    "last_name": {
                        "type": "string",
                        "default": "'Baniya'"
                    },
                    "user_id": {
                        "type": [ "integer", "string" ],
                        "maxLength": 64
                    }
                }
            },
            "description": "Handle test route",
            "model": "test_route",
            "operation": "custom",
            "handler_key": "print:check"
        },
        {
            "route_uri": "/render-html",
            "route_method": "GET",
            "description": "Render HTML view",
            "model": "test_route",
            "operation": "custom",
            "handler_key": "view-html"
        },
        {
            "route_uri": "/render-json",
            "route_method": "GET",
            "description": "Render JSON view",
            "model": "test_route",
            "operation": "custom",
            "handler_key": "view-json"
        },
        {
            "route_uri": "/render-html-content",
            "route_method": "GET",
            "description": "Render raw HTML content",
            "model": "test_route",
            "operation": "custom",
            "handler_key": "view-html-content"
        },
        {
            "route_uri": "/print",
            "route_method": "GET",
            "description": "Handles print",
            "model": "print",
            "operation": "custom",
            "handler_key": "print:check"
        },
        {
            "route_uri": "/command-run",
            "route_method": "POST",
            "description": "Handles command run",
            "model": "print",
            "operation": "custom",
            "handler_key": "command:run"
        }
    ]
}
